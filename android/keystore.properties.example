# Keystore configuration for signing Android builds
# Copy this file to keystore.properties and fill in your actual values
# NEVER commit keystore.properties to git!

# Dev keystore (для development builds)
devStoreFile=dev.keystore
devStorePassword=CHANGE_ME
devKeyAlias=dev
devKeyPassword=CHANGE_ME

# Prod/Release keystore (для production builds)
releaseStoreFile=release.keystore
releaseStorePassword=CHANGE_ME
releaseKeyAlias=release
releaseKeyPassword=CHANGE_ME

